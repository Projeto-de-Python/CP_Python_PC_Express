{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "lint:strict": "eslint . --ext .js,.jsx --max-warnings 0 --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "lint:all": "npm run lint && npm run format:check",
    "validate": "cd .. && python scripts/validate_code_quality.py",
    "validate:python": "cd .. && python -m black --check app/ scripts/ --line-length 100 && python -m isort --check-only app/ scripts/ --profile black",
    "validate:js": "npm run format:check && npm run lint",
    "validate:js:strict": "npm run format:check && npm run lint:strict",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "quality:check": "npm run lint:strict && npm run format:check && npm run audit",
    "quality:fix": "npm run lint:fix && npm run format && npm run audit:fix",
    "fix:all": "cd .. && python -m black app/ scripts/ --line-length 100 && python -m isort app/ scripts/ --profile black && cd frontend && npm run quality:fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-charts": "^8.10.0",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.2",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/css": "^0.11.0",
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.2",
    "esbuild": "^0.21.5",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "vite": "^5.4.19"
  }
}
